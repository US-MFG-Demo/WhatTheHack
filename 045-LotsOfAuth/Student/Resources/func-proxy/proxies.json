{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
    "computation": {
      "matchCondition": {
        "route": "computation"
      },
      "backendUri": "https://%COMPUTATION_PROXY_URL%/api/computation"
    },
    "financial": {
      "matchCondition": {
        "route": "financial"
      },
      "backendUri": "https://%FINANCIAL_PROXY_URL%/api/financial"
    },
    "hr": {
      "matchCondition": {
        "route": "hr"
      },
      "backendUri": "https://%HR_SYSTEM_PROXY_URL%/api/user"
    },
    "weather": {
      "matchCondition": {
        "route": "weather"
      },
      "backendUri": "https://%WEATHER_PROXY_URL%/api/weather"
    },
    "database": {
      "matchCondition": {
        "route": "database"
      },
      "backendUri": "https://%DATABASE_PROXY_URL%/api/database"
    }
  }
}